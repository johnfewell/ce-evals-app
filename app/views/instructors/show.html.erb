  <h2> <%= @instructor.title %> <%= @instructor.fullname %>, <%= @instructor.suffix %> </h2>
    <h4>Courses</h4>
    <div class="mx-5">

    <ul>
        <% @instructor.courses.each do |c| %>
        <li>
          <%= link_to c.title, course_path(c), itemprop: "name codeRepository" %>
        </li>
        <% end %>
    </ul>
  </div>
  <br />


    <%= link_to 'Edit', edit_instructor_path(@instructor) %> |
    <%= link_to 'Back', instructors_path %>
    <%= link_to 'Destroy', @instructor, method: :delete, data: { confirm: 'Are you sure?' } %>
