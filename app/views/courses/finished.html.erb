<h2>Finished Courses</h2>
<ul>
  <% Course.complete.each do |c| %>
  <li class="col-12 d-block width-full py-4 border-bottom public source" itemprop="owns" itemscope="" itemtype="http://schema.org/Code">
      <div class="d-inline-block mb-1">
        <h3>
          <%= link_to c.title, course_path(c), itemprop: "name codeRepository" %>
        </h3>
      </div>
      <div class="f6 text-gray mt-2">
            <span class="repo-language-color ml-0" style="background-color:#701516;"></span>
            <span class="mr-3" itemprop="programmingLanguage">
              Instructor: <%= c.instructor.title %> <%= c.instructor.fullname %>, <%= c.instructor.suffix %>
            </span>
          Date: <%= c.start_date %>
      </div>
    </li>
  <% end %>
</ul>
