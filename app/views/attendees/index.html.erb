  <div class="container">
    <%= link_to "New Attendee", new_attendee_path, id: "news", class: "btn btn-primary" %>
    <div class="columns">
      <% @attendees.each_slice(10) do |attendees| %>
          <div class="one-fifth column">
            <table class="data-table">
              <tr>
                <th>Name</th>
              </tr>
                <% attendees.each do |a| %>
                  <tr>
                    <td>
                        <%= link_to a.fullname, attendee_path(a) %>
                    </td>
                  </tr>
                <% end %>
          </table>
        </div>
      <% end %>
    </div>
  </div>
<br />
<br />
  <h2>Import Attendees</h2>
  <%= form_tag import_attendees_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
